{
  "name": "ui-components-starter",
  "author": "Dhaval",
  "version": "0.1.0",
  "description": "React UI Components Starter",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/cjs/index.d.ts",
  "files": [
    "lib/"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "ui-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dhavalptl/ui-components-starter.git"
  },
  "scripts": {
    "clear": "rm -rf ./lib",
    "start": "tsc --watch",
    "build": "npm run clear && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "test": "jest --coverage",
    "lint": "eslint src --ext .tsx,.ts,.js",
    "lint:fix": "eslint src --ext .tsx,.ts,.js --fix",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 5000"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.9",
    "@storybook/addon-essentials": "^6.0.9",
    "@storybook/addon-links": "^6.0.9",
    "@storybook/react": "^6.0.9",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  }
}
